layout:
    sort:
        games:
            name: asc
        scores:
            score: desc

    group:
        scores:
          - ship
          - mode
          - weapon
          - version

    templates:
        main: |
            {{ description|raw }}
            __NOTOC__
            {{ include('games') }}
        games: |
            {% for game in games %}
            {% if game.template %}
            {{ game.template|raw }}

            {% else %}
            {{ include('game') }}

            {% endif %}
            {% endfor %}
        game: |
            {| class="wikitable" style="text-align: center
            |-
            ! colspan="{{ game.headers|length }}" | {{ game.properties.name }}
            |-
            ! {{ game.headers|join(' !! ') }}
            {% for columns in game.scores %}
            |-
            | {{ columns|join(' || ') }}
            {% endfor %}
            |}


