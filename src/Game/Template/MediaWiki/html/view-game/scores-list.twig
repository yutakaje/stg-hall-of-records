<h3>{{ 'labels.scores'|trans }} ({{ scores|length }})</h3>

<scores>
<table class="wikitable">
  <tr>
    <th>{{ 'labels.player'|trans }}</th>
    <th>{{ 'labels.score'|trans }}</th>
    <th>{{ 'labels.sources'|trans }}</th>
  </tr>
{% for entry in scores %}
  {{ entry|raw }}
{% endfor %}
</table>
</scores>
